msgid "[Important] Cloud Billing Alert"
msgstr "[重点关注]云平台账单消费提醒"

#: cloud_billing/services/notification_service.py:83
#, python-format
msgid ""
"%(provider_name)s current total cost %(current_cost).2f %(currency)s exceeds "
"threshold %(threshold).2f %(currency)s\n"
"Previous hour cost: %(previous_cost).2f %(currency)s"
msgstr ""
"%(provider_name)s 当前总消费 %(current_cost).2f %(currency)s 超过阈值 "
"%(threshold).2f %(currency)s\n"
"上一小时费用: %(previous_cost).2f %(currency)s"

#: cloud_billing/services/notification_service.py:97
#, python-format
msgid ""
"%(provider_name)s account billing increased by %(increase_cost).2f "
"%(currency)s in the last hour, growth rate: %(increase_percent).2f%%\n"
"Current cost: %(current_cost).2f %(currency)s\n"
"Previous hour cost: %(previous_cost).2f %(currency)s"
msgstr ""
"%(provider_name)s 账户在过去一小时的消费增长了 %(increase_cost).2f "
"%(currency)s，增长率为 %(increase_percent).2f%%\n"
"当前费用: %(current_cost).2f %(currency)s\n"
"上一小时费用: %(previous_cost).2f %(currency)s"

